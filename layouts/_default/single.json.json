{{ $originalContext := . }}

{
  "linked_schemas":["organizations_schema-v1.0.0"],
  "primary_url": "{{ .Params.Primary_url }}",
  "name": "{{ .Params.Name }}",
  "description": "{{ .Params.Description }}",
  {{ if .Params.Latitude }}"geolocation": {"lat": {{ .Params.Latitude }}, "lon": {{ .Params.Longitude }}},{{ end }}
  "tags": [{{ range $index2, $tag := .Params.Tags }}{{ with $originalContext.Params.Tags }}"{{ $tag }}"{{ if lt $index2 (sub (len $originalContext.Params.Tags) 1) }},{{ end }}{{ end }}{{ end }}],
  "urls": [{{ range $index2, $url := .Params.urls }}{{ with $originalContext.Params.Tags }}{"name": "{{ $url.name }}", "url": "{{ $url.url }}" }{{ if lt $index2 (sub (len $originalContext.Params.urls) 1) }},{{ end }}{{ end }}{{ end }}],
  "image": "{{ .Params.Image }}",
  "header_image": "{{ .Params.Header_Image }}",
}
